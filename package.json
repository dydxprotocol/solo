{
  "name": "@dolomite-exchange/dolomite-margin",
  "version": "0.8.0",
  "description": "Ethereum Smart Contracts and TypeScript library used for the DolomiteMargin trading protocol",
  "engines": {
    "npm": "6.14.x"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/build/published_contracts/",
    "dist/build/wrappers/",
    "dist/src/",
    "build/contracts/",
    "contracts/external/",
    "contracts/protocol/",
    "LICENSE",
    "README.md"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "scripts": {
    "test": "npm run deploy_test && npm run test_only",
    "build": "npm run compile -- --all && npm run clean_contract_json && npm run build:wrappers && npm run build:test_wrappers && npm run build:js",
    "compile": "truffle compile",
    "build:wrappers": "typechain --target=web3-1.0.0 --outDir=build/wrappers/ './build/published_contracts/*.json'",
    "build:test_wrappers": "typechain --target=web3-1.0.0 --outDir=build/testing_wrappers/ './build/testing_contracts/*.json'",
    "migrate": "truffle migrate",
    "develop": "truffle develop",
    "console": "truffle console",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:sol && npm run lint:dolomite-margin",
    "lint:ts": "tslint --project . -t verbose 'src/**/*.ts' '__tests__/**/*.ts'",
    "lint:js": "eslint --ext .js --ignore-path .gitignore migrations/**/*.js",
    "lint:sol": "solium --debug --fix -d contracts/",
    "lint:dolomite-margin": "python util/lintcontracts.py",
    "coverage": "node scripts/coverage.js",
    "node": "ganache-cli",
    "debug": "truffle debug",
    "build:js": "rm -rf dist/ && node --max-old-space-size=8192 node_modules/.bin/tsc && cp -r build/wrappers dist/build",
    "build:cov": "npm run clean_contract_json && npm run build:wrappers && npm run build:test_wrappers",
    "replace_bytecode": "ts-node ./scripts/ReplaceCoverageBytecode.ts",
    "docker_node": "ganache-cli -i 1313 -d -p 8545 -h 0.0.0.0 --db=/home/.ganache",
    "check_amm_bytecode": "ts-node ./scripts/CheckAmmBytecode.ts",
    "clean_contract_json": "ts-node ./scripts/CleanBuild.ts",
    "fix_deployment_file": "ts-node ./scripts/FixDeploymentFile.ts",
    "deploy": "npm run fix_deployment_file && NODE_ENV=production truffle migrate --network=$NETWORK --reset && npm run save_deployed_addresses",
    "deploy_kovan": "NETWORK=kovan npm run deploy",
    "deploy_mainnet": "NETWORK=mainnet npm run deploy",
    "deploy_matic": "NETWORK=matic npm run deploy",
    "deploy_mumbai": "NETWORK=mumbai_matic npm run deploy",
    "deploy_arbitrum": "NETWORK=arbitrum npm run deploy",
    "deploy_arbitrum_rinkeby": "NETWORK=arbitrum_rinkeby npm run deploy",
    "deploy_test": "npm run reset_test_evm && npm run migrate -- --network test --reset && npm run snapshot_test_evm && npm run check_amm_bytecode && npm run clean_contract_json",
    "deploy_coverage": "RPC_NODE_URI=http://127.0.0.1:8555 npm run reset_test_evm && COVERAGE=true npm run migrate -- --network coverage && RPC_NODE_URI=http://127.0.0.1:8555 npm run snapshot_test_evm && npm run clean_contract_json",
    "deploy_test_ci": "RPC_NODE_URI=http://0.0.0.0:8545 npm run reset_test_evm && npm run migrate -- --network test_ci --reset && RPC_NODE_URI=http://0.0.0.0:8545 npm run snapshot_test_evm && npm run check_amm_bytecode && npm run clean_contract_json",
    "test_only": "NODE_ENV=test jest --setupFiles dotenv/config --runInBand",
    "test_only_specific": "NODE_ENV=test jest --setupFiles dotenv/config --runInBand -t 'AddManyMarkets'",
    "test_cov": "npm run deploy_coverage && NETWORK_ID=1002 COVERAGE=true RPC_NODE_URI=http://127.0.0.1:8555 npm run test_only -- --forceExit",
    "ci_test": "NODE_ENV=test RPC_NODE_URI=http://0.0.0.0:8545 jest --setupFiles dotenv/config --runInBand",
    "reset_test_evm": "node scripts/reset-test-evm.js",
    "snapshot_test_evm": "node scripts/snapshot-test-evm.js",
    "install_compiler": "docker pull ethereum/solc:0.5.7",
    "save_deployed_addresses": "ts-node ./scripts/SaveDeployedAddresses.ts",
    "serve_docs": "docsify serve ./docs",
    "execute-set-expiry": "ts-node scripts/SetExpiry.ts",
    "verify": "DEPLOYER_PRIVATE_KEY=0x1234567812345678123456781234567812345678123456781234567812345678 NODE_ENV=production ts-node scripts/VerifyContracts.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dolomite-exchange/dolomite-margin"
  },
  "keywords": [
    "dolomite",
    "defi",
    "dydx",
    "uniswap",
    "ethereum"
  ],
  "author": "Dolomite",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dolomite-exchange/dolomite-margin/issues"
  },
  "homepage": "https://github.com/dolomite-exchange/dolomite-margin#readme",
  "dependencies": {
    "@openzeppelin/contracts": "^2.5.1",
    "@types/web3": "1.0.18",
    "@uniswap/lib": "^4.0.1-alpha",
    "axios": "^0.21.1",
    "bignumber.js": "^8.1.1",
    "canonical-weth": "^1.4.0",
    "dotenv": "^16.0.0",
    "es6-promisify": "^6.0.1",
    "ethers": "4.0.39",
    "query-string": "^6.9.0",
    "web3": "1.2.1"
  },
  "devDependencies": {
    "@poanet/solidity-flattener": "^3.0.6",
    "@truffle/config": "^1.2.14",
    "@truffle/hdwallet-provider": "^2.0.6",
    "@types/axios": "^0.14.0",
    "@types/es6-promisify": "^6.0.0",
    "@types/jest": "^24.0.9",
    "@types/node": "^10.12.30",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.3",
    "docsify-cli": "^4.3.0",
    "dotenv-flow": "^0.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "ethlint": "^1.2.5",
    "ganache-cli": "^6.12.2",
    "jest": "24.1.0",
    "mocha-junit-reporter": "^1.18.0",
    "ora": "^4.0.3",
    "solidity-coverage": "0.7.1",
    "truffle": "~5.1.12",
    "truffle-flattener": "^1.5.0",
    "truffle-plugin-verify": "^0.5.21",
    "ts-jest": "24.1.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-no-focused-test": "^0.5.0",
    "typechain": "^0.3.11",
    "typescript": "^3.8.3"
  }
}
